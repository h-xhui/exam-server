<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.exam.dao.QuestionTagMapper" >

    <insert id="addQuestionTag" parameterType="com.exam.pojo.entity.QuestionTag">
        insert into question_tag(question_id,tag_id)
            value(#{questionId},#{tagId});
    </insert>
</mapper>