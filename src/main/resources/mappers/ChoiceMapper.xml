<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.exam.dao.ChoiceMapper" >


    <insert id="addChoice" parameterType="com.exam.pojo.entity.Choice">
        insert into choice(id,question_id,content,ans )
            value(null,#{questionId},#{content},#{ans});
    </insert>
</mapper>